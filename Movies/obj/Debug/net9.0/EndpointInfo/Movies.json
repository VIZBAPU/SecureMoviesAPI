  "components": {
    "schemas": {
      "AuthenticationRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Movie": {
        "required": [
          "director",
          "genre",
          "rating",
          "releaseYear",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "director": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "releaseYear": {
            "maximum": 2030,
            "minimum": 1900,
            "type": "integer",
            "format": "int32"
          },
          "genre": {
            "maxLength": 30,
            "minLength": 0,
            "type": "string"
          },
          "rating": {
            "maximum": 10,
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
